<h1>Add enrollments</h1>
<%= form_with(model: [@course, @enrollment], local: :true) do |f|%>
  <%= f.label :user%>
  <%= f.select :user_id, @users.collect { |u| [u.full_name, u.id] }%>
  <br />
  <%= f.label :role%>
  <%= f.select :role, ['ta','teacher','student']%>
  <br />
  <%= f.submit nil, class:'button'%>
<% end %>
